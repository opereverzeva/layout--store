.contact{
    background-color var(--main-color)
    height 316px
    &__map{
        height 213px
    }
    &__iframe{
        width: 100%
        height 213px
    }
    &__info{
        height 103px
        margin-left 24px
    }
    &__title{
        color: var(--main-text-color)
        font-size: 22px
        line-height: 32px
        font-weight 900
        height 40px
        margin 7px 0 0 0 
    }
    &__address{
        width 213px
        margin-bottom: 18px
        font-size: 15px
        line-height: 20px
        font-style normal

    }
}